  "version": "0.2.0",
  "configurations": [
    {
      "type": "haxe-eval",
      "name": "Launch Haxe Eval",
      "request": "launch"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/mcb.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch dts",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/util/dtsgen.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Tests",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/testbed.js"
    }
  ]
}
