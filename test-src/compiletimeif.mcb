function fizzbuzz{
    LOOP(0,100) as i{
        IF i % 15 == 0 {
            say FizzBuzz
        } ELSE IF i % 3 == 0 {
            say Fizz
        } ELSE IF i % 5 == 0 {
            say Buzz
        } ELSE {
            say <%i%>
        }
    }
}

dir tld{
    LOOP(0,2) as i {
        dir <%i%> {
            IF i == 0 {
                function test{
                    say a
                }
            } ELSE {
                function test{
                    say b
                }
            }
        }
    }
}