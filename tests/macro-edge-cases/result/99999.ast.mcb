### main.mcb
function test{
	$execute if score a b = 2 run { 
		say 1
	} else $execute if score a b = 3 run { 
		say 2
	} else $run { 
		say 3
	}
	$block{ {awd: 1}
		say hi
	}
	$block{ {awd: 2, asd: $(value)}
		$say $(asd)
	}
	$schedule 1t append {
		say hi
	}
	$schedule 1t append {
		say hi
	}
	$schedule $(delay)t append {
		say hi
	}
	$schedule 2t append {
		say hi
	}
}
