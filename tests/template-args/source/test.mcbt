template with-args{
    with a:int b:int c:int {
        say a=<%a%>
        say b=<%b%>
        say c=<%c%>
    }
    with a:int b:raw {
        say a=<%a%> (<%typeof a%>)
        say b=<%b%> (<%typeof b%>)
    }
}

template without-args {
    with {
        say this has no arguments
    }
}

template arg-js {
    with a:js {
        REPEAT(a) as item{
            say item=<%item%> (<%typeof item%>)
        }
    }
}

template arg-block {
    with a:block {
        <%embed(a)%>
    }
}

template arg-literal{
    with test {
        say test
    }
    with bar {
        say bar
    }
}

template js-cache{
    with a:int {
        say no
    }
    with b:js {
        say <%JSON.stringify(b)%>
    }
}