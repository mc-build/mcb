dir test{
    function int-int{
        REPEAT(1,10) as i {
            say <%i%>
        }
    }

    function float-float-float{
        REPEAT(1.0,10.0,0.5) as i {
            say <%i%>
        }
    }
    
    function array {
        REPEAT([1,2,7,9]) as i {
            say <%i%>
        }
    }

    function iterator-function{
        REPEAT(iteratorFunction) as i{
            say <%i%>
        }
    }
}